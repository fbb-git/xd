void special()
{
    if ("INSTALL.im" newer "parser/SKEL")
        run("echo \"#define _Skel_  \\\"" + SKEL + "\\\"\" > parser/SKEL");

    if (! exists("release.yo") || "VERSION" newer "release.yo") 
    {
        system("touch version.cc");
        run("gcc -E VERSION.h | grep -v '#' | sed 's/\\\"//g' > release.yo");
    }
}
